{
  "name": "hidden-space-client",
  "version": "1.0.0",
  "description": "TheHiddenSpace Website",
  "author": "Liam Potter <me@liam-potter.co.uk>",
  "private": true,
  "scripts": {
    "dev": "cross-env HOST=0.0.0.0 PORT=3002 NODE_ENV=development nuxt",
    "dev:production": "cross-env DEBUG=nuxt:* HOST=0.0.0.0 PORT=3002 NODE_ENV=production nuxt",
    "build": "cross-env DEBUG=nuxt:* NODE_ENV=production nuxt build --modern=server",
    "start": "cross-env HOST=0.0.0.0 PORT=3002 NODE_ENV=production nuxt start",
    "start:pm2": "pm2 start npm --name \"hidden-space-client\" -- start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.10.1",
    "@fortawesome/pro-light-svg-icons": "^5.10.1",
    "@fortawesome/pro-regular-svg-icons": "^5.10.1",
    "@fortawesome/pro-solid-svg-icons": "^5.10.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/markdownit": "^1.2.6",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "date-fns": "2.0.0-beta.4",
    "konva": "^4.0.0",
    "nuxt": "npm:nuxt-edge",
    "portal-vue": "^2.1.6",
    "vue-konva": "^2.0.7"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "standard": "^13.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.4"
  }
}
