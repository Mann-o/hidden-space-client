@font-face
  font-display: swap
  font-family: 'Poppins'
  font-style: normal
  font-weight: 300
  src: local('Poppins Light'), local('Poppins-Light'),
       url('/fonts/poppins-v6-latin-300.woff2') format('woff2'),
       url('/fonts/poppins-v6-latin-300.woff') format('woff')

@font-face
  font-display: swap
  font-family: 'Poppins'
  font-style: italic
  font-weight: 300
  src: local('Poppins Light Italic'), local('Poppins-LightItalic'),
       url('/fonts/poppins-v6-latin-300italic.woff2') format('woff2'),
       url('/fonts/poppins-v6-latin-300italic.woff') format('woff')

